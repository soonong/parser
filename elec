import requests
from bs4 import BeautifulSoup
import json
import pandas as pd

req = requests.get("http://www.keca.or.kr/ecic/ad/ad0101D.do?menuCd=6047&currentPageNo=1&license=13020662&gubun=detail&sido=51&hoeyn=&searchSido=&searchSigungu=&searchGubun=company&searchText=&CSRFToken=8ab9292a-389d-40b3-979a-4a7815959e3a")
cont = req.content
soup = BeautifulSoup(cont,"html.parser")

bbs = soup.find("div",{"class":"bbs"})
tds = bbs.findAll("tr")

lists = []
for td in tds:
    nums = td.findAll("td")
    for num in nums:
        wwww = num.text
        dict = {wwww}        # 여기에 키값이랑 벨류값 입력하려면, td의 각각의 값을 정의해줘야한느것으로 이해했는데..
        lists.append(wwww)   # 같은 태그에 여러값이 들어있어서 .. 어떻게 하는지 못찾겠습니다.

file = open("./elecList.json","w+")
file.write(json.dumps(lists))

df = pd.read_json('./elecList.json')
print(df)
